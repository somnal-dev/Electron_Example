{
  "name": "electron-example",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "main": "public/electron.ts",
  "homepage": ".",
  "scripts": {
    "react-start": "vite",
    "react-build": "tsc -b && vite build",
    "start": "concurrently \"cross-env NODE_ENV=development BROWSER=none yarn react-start\" \"wait-on http://localhost:5173 && electron .\"",
    "build": "yarn react-build && electron-builder",
    "build:win": "yarn react-build && electron-builder --win --x64",
    "lint": "eslint .",
    "preview": "vite preview"
  },
  "dependencies": {
    "electron-is-dev": "^3.0.1",
    "react": "^19.1.1",
    "react-dom": "^19.1.1",
    "robotjs": "^0.6.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.33.0",
    "@types/electron": "^1.6.12",
    "@types/react": "^19.1.10",
    "@types/react-dom": "^19.1.7",
    "@vitejs/plugin-react": "^5.0.0",
    "concurrently": "^9.2.1",
    "cross-env": "^10.0.0",
    "electron": "^38.1.0",
    "electron-builder": "26.0.12",
    "electron-rebuild": "^3.2.9",
    "eslint": "^9.33.0",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.20",
    "globals": "^16.3.0",
    "typescript": "~5.8.3",
    "typescript-eslint": "^8.39.1",
    "vite": "^7.1.2",
    "wait-on": "^8.0.5"
  },
  "packageManager": "yarn@4.9.4"
}
